{
  "name": "socketshooter",
  "version": "1.0.0",
  "description": "a simple 2D competitive multiplayer shooter made with free art",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "server": "ts-node src/server/main.ts",
    "server-dev": "ts-node-dev --watch \"src/**/*\" --respawn --transpile-only src/server/main.ts",
    "server-prod": "node dist/main.js",
    "client": "vite",
    "client-build": "tsc && vite build",
    "preview": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dirtOval/SocketShooter.git"
  },
  "author": "deirdre loyall",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dirtOval/SocketShooter/issues"
  },
  "homepage": "https://github.com/dirtOval/SocketShooter#readme",
  "dependencies": {
    "colyseus": "^0.14.24",
    "express": "^4.18.2",
    "phaser": "^3.55.2",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4"
  }
}
